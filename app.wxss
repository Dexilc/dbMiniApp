@font-face {
  font-family: "iconfont";
  src: url('./font/iconfont.eot?t=1591957947584');
  /* IE9 */
  src: url('./font/iconfont.eot?t=1591957947584#iefix') format('embedded-opentype'),
    /* IE6-IE8 */
    url('data:application/x-font-woff2;charset=utf-8;base64,') format('woff2'),
    url('./font/iconfont.woff?t=1591957947584') format('woff'),
    url('./font/iconfont.ttf?t=1591957947584') format('truetype'),
    /* chrome, firefox, opera, Safari, Android, iOS 4.2+ */
    url('./font/iconfont.svg?t=1591957947584#iconfont') format('svg');
  /* iOS 4.1- */
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-zoom:before {
  content: "\e674";
}

.icon-ico-right-arrow:before {
  content: "\e66c";
}

.ovrhidden {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

/* 隐藏scroll滚动条 */
::-webkit-scrollbar {
  display: none;
  width: 0;
  height: 0;
  color: transparent;
}